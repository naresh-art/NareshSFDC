<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>phonenumber_length</fullName>
    <active>false</active>
    <errorConditionFormula>LEN( Customer_Number__c ) &gt; 10  || LEN( Customer_Number__c ) &lt; 10</errorConditionFormula>
    <errorDisplayField>Customer_Number__c</errorDisplayField>
    <errorMessage>Please Enter 10 digits phone number</errorMessage>
</ValidationRule>

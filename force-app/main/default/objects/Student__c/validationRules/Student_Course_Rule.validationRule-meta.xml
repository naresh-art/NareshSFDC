<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Student_Course_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISPICKVAL( Course__c, &quot;Salesforce&quot; )),
Course_Fee__c &lt;= 5000
)</errorConditionFormula>
    <errorMessage>Enter Salesforce course fees less than 5000</errorMessage>
</ValidationRule>
